package com.java.test.program.interview;

public class Node{
	
	private Node next;
	private String data;
	
	Node(String data)
	{
		this.data = data;
	}
	
	public void setData(String data)
	{
		this.data = data;
	}
	
	public String getDate()
	{
		return this.data;
	}
	
	public void setNext(Node node)
	{
		this.next = node;
	}
	public Node getNext()
	{
		return this.next;
	}
	
	
	
}
