package testinterview.zipranges;

import java.util.ArrayList;
import java.util.Comparator;
import java.util.List;

public class ZipRanges {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		ArrayList<List<Integer>> collection = new ArrayList<List<Integer>>();
		List zipRange1 = new ArrayList<Integer>();
		zipRange1.add(201);
		zipRange1.add(250);
		List zipRange2 = new ArrayList<Integer>();
		zipRange2.add(254);
		zipRange2.add(260);
		List zipRange3 = new ArrayList<Integer>();
		zipRange3.add(245);
		zipRange3.add(249);
		List zipRange4 = new ArrayList<Integer>();
		zipRange4.add(256);
		zipRange4.add(300);
		List zipRange5 = new ArrayList<Integer>();
		zipRange5.add(260);
		zipRange5.add(304);
		
		collection.add(zipRange1);
		collection.add(zipRange2);
		collection.add(zipRange3);
		collection.add(zipRange4);
		collection.add(zipRange5);
		
		
		collection.sort(new Comparator<List<Integer>>() {
			@Override
			public int compare(List<Integer> o1, List<Integer> o2) {
				return  o1.get(0) - o2.get(0)  ;
			}
		});	
		
		for(List<Integer> item : collection)
		{
			System.out.println(item);
		}
		

	}

	
	
	protected List<List<Integer>> getZipRangesCollection(List<List<Integer>> zipRangesCollection)
	{
		List<List<Integer>> zipColletion = new ArrayList<List<Integer>>();
		
		

			
		return zipColletion;
	}
	
}
