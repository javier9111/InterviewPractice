package testinterview.datastructures.hashmaps;

public class ReverseLionkedList {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int num=321;
		int rev=0;
		int dig = 0;
		while(num>0){
			dig=num%10;
			rev=rev*10+dig;
			num=num/10;
		}
		System.out.println(num);
	}

}
