package com.java.test.program.interview;

public class Fibonacci {

	
	
	public static void main(String[] args)
	{
		
		
		int counter =5;
		int prev = 0;
		int fibonacci=0;
		for(int i =1; i<=counter; i++)
		{
			fibonacci = fibonacci+prev;
			System.out.println(" fib:"+ fibonacci+" prev:"+prev);
			prev = i;
		}
	}
}
