package LeetCodeExc.medium;

public class ReverseInteger {

	
	
	
	
	public static void main(String[] args){
		
		
		int x1 =123;
		
		
		System.out.println(reverse(x1));
		
	}
	
	
	
	public static int reverse(int x){
		
		int tempX = x;
		int sum=0;
		
		while(tempX>0){
			
			int single = tempX%10;
			
			sum=sum*10+single;
			
			tempX =tempX/10;
		}
		
		return sum;
	}
}
