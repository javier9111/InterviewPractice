    static int sockMerchant(int n, int[] ar) {
    	
        Map<Integer, Integer> colorMatches = new HashMap();
        
        for(int i = 0; i<ar.length; i++)
        {
        	colorMatches.put(ar[i], colorMatches.get(ar[i])+1);
        }

    }

