    public int getPrimePalindrome(int N) {
        
    	for(int i=N; i<(2*10^8); i++)
    	{
    		
    		if(isPrime(i))
    		{
    			if(isPalindrome(i))
    			{
    				return i;
    			}
    		}
    	}
    	
    	return -1;
    }


	private boolean isPrime(int i) {
		// TODO Auto-generated method stub
		return false;
	}


	private boolean isPalindrome(int i) {
		// TODO Auto-generated method stub
		return false;
	}
}
