package main;

public class AutoEncender implements IEstadoAuto {
	
	private final Auto auto;
	
	public AutoEncender(Auto auto) {
		this.auto = auto;
	}
	
	public void encender() {
		System.out.println("El auto esta encendido!");
	}
	
	public void manejar() {
		
	}
	
	public void apagar() {
		
	}
	
}
