package main;

public class Enemigo {

	private String imagen; //
	private int posX;
	private int posY;
	
	public Enemigo(String imagen, int posX, int posY) {
		this.setImagen(imagen);
		this.setPosX(posX);
		this.setPosX(posX);
	}
	
	public Enemigo clone() {
		return new Enemigo(this.imagen, this.posX, this.posY);
	}
	
	public String getImagen() {
		return imagen;
	}
	
	public void setImagen(String imagen) {
		this.imagen = imagen;
	}
	
	public int getPosX() {
		return posX;
	}
	
	public void setPosX(int posX) {
		this.posX = posX;
	}
	
	public int getPosY() {
		return posY;
	}
	
	public void setPosY(int posY) {
		this.posY = posY;
	}
	
}
