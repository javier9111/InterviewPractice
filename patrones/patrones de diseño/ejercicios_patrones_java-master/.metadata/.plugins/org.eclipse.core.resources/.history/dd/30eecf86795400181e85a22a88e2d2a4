package main;

public class Main {

	public static void main(String[] args) {
		Auto auto = new Auto();
	    
		auto.apagar();
		auto.encender();
		auto.apagar();
		auto.encender();
		auto.manejar();
		auto.apagar();
		
	    /*
	     * Encendido
	     * En Movimiento
	     * Apagado
	     * */
	}

}
