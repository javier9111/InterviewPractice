package main;

import java.util.*;

public class Producto {
	
	private int stock;
	
	public Producto(int stock) {
		this.stock = stock;
	}
	
	public void venta() {
		this.setStock(this.stock - 1);
		
	}

	public int getStock() {
		return stock;
	}

	public void setStock(int stock) {
		this.stock = stock;
	}
}
