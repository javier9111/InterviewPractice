package main;

public class TV implements IDevise{
	
	private boolean on;
	
	public 
	
	public void on() {
		this.on = true;
		System.out.println("TV encendida!");
	}
	
	public void off() {
		this.on = true;
		System.out.println("TV apagada!");
	}
	
	private void test() { //Evitamos esto <---
		on();
		off();
		on();
		off();
		on();
		off();
	}
	
}
